{
  "name": "market-analysis-bot",
  "version": "1.0.0",
  "description": "AI駆動自動市場分析システム",
  "main": "auto_market_analysis.js",
  "scripts": {
    "test": "node -e 'const { collectAllMarketData } = require(\"./market_data_collector.js\"); collectAllMarketData().then(data => console.log(\"✅ データ収集テスト成功:\", data)).catch(console.error)'",
    "manual": "node auto_market_analysis.js",
    "start": "node scheduler.js",
    "build": "eleventy",
    "dev": "eleventy --serve",
    "clean": "rm -rf _site",
    "pm2:setup": "node -e \"console.log('Setting up PM2 for Deep Research...'); require('child_process').exec(process.platform === 'win32' ? 'scripts\\\\pm2-setup.bat' : 'bash scripts/pm2-setup.sh', (e,o,err) => { if(e) console.error(e); else console.log(o); })\"",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop deep-research-scheduler",
    "pm2:restart": "pm2 restart deep-research-scheduler",
    "pm2:status": "pm2 status",
    "pm2:logs": "pm2 logs deep-research-scheduler",
    "pm2:monit": "pm2 monit"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "axios": "^1.11.0",
    "axios-retry": "^3.9.1",
    "dotenv": "^16.6.1",
    "handlebars": "^4.7.8",
    "moment": "^2.29.4",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "market-analysis",
    "automation",
    "gemini",
    "trading",
    "finance",
    "ssg",
    "static-site-generator"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yama-sansan777/market-analysis-AI.git"
  },
  "bugs": {
    "url": "https://github.com/yama-sansan777/market-analysis-AI/issues"
  },
  "homepage": "https://github.com/yama-sansan777/market-analysis-AI#readme"
}
